{
    "Policy": {
        "PolicyName": "bar-logs-lambda-ENV",
        "Path": "/"
    },
    "Document" : {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Action": [
                    "logs:CreateLogGroup",
                    "logs:CreateLogStream",
                    "logs:PutLogEvents"
                ],
                "Effect": "Allow",
                "Resource": "arn:aws:logs:*:*:*"
            }
        ]
    }
}